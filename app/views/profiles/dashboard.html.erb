<div class="container" id="user-dashboard-button">
  <%= render 'shared/dashboard' %>
<% if @user %>
  <%= form_for @user, url: edit_user_registration_path(@user), method: :get do |f| %>
    <%= f.hidden_field :recipes, value: [] %>
    <%= f.submit "Edit your profile", class: "btn btn-primary", id: "match_button" %>
  <% end %>
<% end %>
</div>
